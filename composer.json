{
    "name": "armincms/sofre",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",  
        "armincms/armincms": "~0.3", 
        "armincms/nova-targomaan": "~0.1",
        "armincms/nova-comment": "~0.1",
        "armincms/nova-opening-hours": "~0.1", 
        "armincms/input-select": "~0.1",
        "armincms/chain": "~0.3",
        "armincms/location": "~0.3",
        "armincms/currency": "~0.1",
        "armincms/categorizable": "~0.1",
        "armincms/taggable": "~0.1", 
        "dillingham/nova-items-field": "~0.1",
        "zareismail/nova-radio-field": "~0.1", 
        "dpsoft/nova-persian-date": "^1.1",
        "spatie/opening-hours": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "Armincms\\Sofre\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Armincms\\Sofre\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
